{
  "name": "@gftdcojp/gftd-orm",
  "version": "0.1.0",
  "description": "Enterprise-grade real-time data platform with ksqlDB, inspired by Supabase architecture",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./browser": {
      "import": "./dist/src/browser-client.js",
      "require": "./dist/src/browser-client.js",
      "types": "./dist/src/browser-client.d.ts"
    },
    "./server": {
      "import": "./dist/src/server-client.js",
      "require": "./dist/src/server-client.js",
      "types": "./dist/src/server-client.d.ts"
    },
    "./hooks": {
      "import": "./dist/src/hooks/useGftdOrm.js",
      "require": "./dist/src/hooks/useGftdOrm.js",
      "types": "./dist/src/hooks/useGftdOrm.d.ts"
    }
  },
  "files": [
    "dist/src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:benchmark": "vitest run --reporter=verbose src/__tests__/benchmark.test.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "demo": "pnpm exec ts-node examples/mock-demo.ts",
    "demo:real": "pnpm exec ts-node examples/demo.ts",
    "demo:gftd": "pnpm exec ts-node examples/gftd-orm-demo.ts",
    "demo:mv": "pnpm exec ts-node examples/materialized-view-demo.ts",
    "prepublishOnly": "pnpm run build && pnpm run test",
    "prepack": "pnpm run build"
  },
  "keywords": [
    "ksqldb",
    "orm",
    "kafka",
    "confluent",
    "typescript",
    "drizzle",
    "stream-processing",
    "realtime",
    "storage",
    "auth",
    "supabase",
    "gftd",
    "nextjs",
    "react",
    "ssr",
    "client-server",
    "hooks"
  ],
  "author": {
    "name": "GFTD Team",
    "url": "https://github.com/gftdcojp"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/gftdcojp/gftd-orm.git"
  },
  "homepage": "https://github.com/gftdcojp/gftd-orm#readme",
  "bugs": {
    "url": "https://github.com/gftdcojp/gftd-orm/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "avsc": "^5.7.7",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "express-slow-down": "^2.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}