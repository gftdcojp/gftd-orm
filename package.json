    "storage",
    "auth",
    "supabase",
    "gftd"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "avsc": "^5.7.7",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
